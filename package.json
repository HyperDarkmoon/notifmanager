{
  "name": "notifmanager",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "core-js": "3.44.0",
    "es6-promise": "4.2.8",
    "object-assign": "4.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "react-scripts": "5.0.1",
    "serve": "^14.2.3",
    "web-vitals": "^2.1.4",
    "whatwg-fetch": "3.6.20"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:local": "copy .env.local .env && react-scripts start",
    "start:production": "copy .env.production .env && react-scripts start",
    "start:network": "set HOST=10.41.15.227&& set HTTPS=false&& react-scripts start",
    "build": "react-scripts build",
    "build:local": "copy .env.local .env && react-scripts build",
    "build:production": "copy .env.production .env && react-scripts build",
    "build:smarttv": "copy .env.production .env && set GENERATE_SOURCEMAP=false&& react-scripts build",
    "serve": "serve -s build -l 3000",
    "serve:network": "serve -s build -l 3000 -L",
    "serve:silent": "serve -s build -l 3000 -L",
    "prestart": "npm config set update-notifier false && npm config set fund false",
    "prebuild": "npm config set update-notifier false && npm config set fund false",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie >= 9",
      "android >= 4",
      "ios >= 8",
      "samsung >= 4",
      "opera >= 15",
      "chrome >= 30",
      "firefox >= 30",
      "safari >= 8"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie >= 9"
    ]
  }
}
